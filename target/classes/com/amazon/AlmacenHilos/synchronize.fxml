<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.amazon.AlmacenHilos.SynchronizeController">
   <center>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="420.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="199.0" layoutY="349.0" mnemonicParsing="false" onAction="#startButton" text="Button" />
            <Label layoutX="44.0" layoutY="59.0" prefHeight="42.0" prefWidth="114.0" text="Correos">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Label layoutX="44.0" layoutY="164.0" prefHeight="42.0" prefWidth="114.0" text="SEUR">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <TextField fx:id="correos" layoutX="251.0" layoutY="67.0" prefHeight="26.0" prefWidth="154.0" promptText="Insertar Numero" />
            <TextField fx:id="seur" layoutX="251.0" layoutY="172.0" prefHeight="26.0" prefWidth="154.0" promptText="Insertar Numero" />
         </children></AnchorPane>
   </center>
   <left>
      <VBox prefHeight="420.0" prefWidth="150.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#switchToJoin" prefHeight="105.0" prefWidth="150.0" text="Join" />
            <Button mnemonicParsing="false" onAction="#switchToSearch" prefHeight="105.0" prefWidth="150.0" text="Search" />
            <Button mnemonicParsing="false" prefHeight="105.0" prefWidth="150.0" text="Synchronize" />
            <Button mnemonicParsing="false" onAction="#switchToShareResources" prefHeight="105.0" prefWidth="150.0" text="Share Resources" />
         </children>
      </VBox>
   </left>
</BorderPane>
