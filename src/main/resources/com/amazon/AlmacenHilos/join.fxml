<BorderPane prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.amazon.AlmacenHilos.JoinController">
   <center>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="420.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="21.0" layoutY="96.0" prefHeight="42.0" prefWidth="114.0" text="Correos">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Label layoutX="21.0" layoutY="157.0" prefHeight="42.0" prefWidth="114.0" text="SEUR">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Label layoutX="21.0" layoutY="227.0" prefHeight="42.0" prefWidth="114.0" text="MRW">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <TextField fx:id="correos" layoutX="259.0" layoutY="104.0" prefHeight="26.0" prefWidth="154.0" promptText="Insertar Numero" />
            <TextField fx:id="seur" layoutX="259.0" layoutY="165.0" prefHeight="26.0" prefWidth="154.0" promptText="Insertar Numero" />
